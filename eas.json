{
  "cli": {
    "version": ">= 13.4.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "YARN_ENABLE_IMMUTABLE_INSTALLS": "false"
      },
      "command": "sudo corepack enable"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "YARN_ENABLE_IMMUTABLE_INSTALLS": "false"
      },
      "command": "sudo corepack enable"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "YARN_ENABLE_IMMUTABLE_INSTALLS": "false"
      },
      "command": "sudo corepack enable"
    }
  },
  "submit": {
    "production": {}
  }
}
